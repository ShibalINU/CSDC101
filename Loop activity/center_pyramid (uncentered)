#include <iostream>
using namespace std;

int main()
{ //center pyramid part
    int num = 10;
    for (int i = 1; i < num; i++){
        for (int j = i; j < num; j++){
            cout << " ";
        }
        for (int j = 1; j < i; j++){
            cout << "* ";
        }
        cout << endl;
    } 

    return 0;
}
